<!--components/w-tab-nav/w-tab-nav.wxml-->
<view class="tab-title">
	<block wx:for="{{titles}}" wx:key="index">
		<view class='tab-item {{currentIndex == index ? "active":" " }}'
      bind:tap="handleIndexClick"
      data-index="{{index}}">
      <text class="text">{{item}}</text>
    </view>
	</block>

</view>